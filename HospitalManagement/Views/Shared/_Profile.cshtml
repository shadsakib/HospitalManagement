<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, minimum-scale=1,  maximum-scale=1">
    <meta name="Description" content="" />
    <meta name="author" content="" />
    <meta name="robots" content="" />
    <meta name="googlebot" content="" />
    <meta name="google" content="nositelinkssearchbox" />
    <link href=@Url.Content("~/Content/Profile/css/styles.css") rel="stylesheet" media="screen">
    <link rel="shortcut icon" type="image/ico" href="@Url.Content("~/Content/Profile/img/favicon.ico")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Template/assets/")css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Template/assets/")css/style.css">


</head>

<body>
    <div id="layout">
        <header style="background:ghostwhite">
            <div class="container">
                <div class="row">
                    <div class="logo">
                        <a href="/Home/Index"><img src="@Url.Content("~/Content/Medino/assets/")images/logo/logo.png" alt="logo"></a>
                    </div>

                    <div class="tools-top">
                        <div class="avatar-profile">
                            <div class="user-edit">
                                <h4 style="color:black">@Session["Username"]</h4>
                                <a style="color:black" href="/Home/PatientAccountInfo">edit profile</a>
                            </div>
                            <div class="avatar-image">
                                <img src="@Url.Content("~/Content/Profile/")img/avatar-profile.jpg" alt="avatar profile" class="img-responsive">
                            </div>
                        </div>
                        <ul class="tools-help">
                            <li><a style="color:black" href="#" title="Help" data-toggle="tooltip" data-placement="bottom"><i class="fa fa-question-circle"></i></a></li>
                            <li><a style="color:black" href="/Home/Logout" title="Logout" data-toggle="tooltip" data-placement="bottom"><i class="fa fa-sign-out"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>

        <nav>
            <div class="container">
                <h4 class="navbar-brand">menu</h4>
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <i class="fa fa-bars" aria-hidden="true"></i>
                    </button>
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li> <i class="fa fa-address-book-o"></i> <a href="/Home/Test"> book appointment</a> </li>
                        <li> <i class="fa fa-files-o"></i> <a href="#">Result Examinations</a> </li>
                        <li> <i class="fa fa-files-o"></i> <a href="/Home/PrescriptionList">Prescriptions</a> </li>
                        <li> <i class="fa fa-pencil"></i> <a href="/Home/PatientAccountInfo">my account</a> </li>
                    </ul>

                </div>
            </div>
        </nav>

        <div> </div>
        @RenderBody()

    </div>

    <script data-cfasync="false" src="@Url.Content("~/Content/Profile/")cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
    <script type="39c83edf3988fce0c205f478-text/javascript" src="@Url.Content("~/Content/Profile/")js/jquery.js"></script>
    <script type="39c83edf3988fce0c205f478-text/javascript" src="@Url.Content("~/Content/Profile/")js/jquery-ui.min.js"></script>
    <script type="39c83edf3988fce0c205f478-text/javascript" src="@Url.Content("~/Content/Profile/")js/bootstrap.min.js"></script>
    <script type="39c83edf3988fce0c205f478-text/javascript" src="@Url.Content("~/Content/Profile/")js/bootstrap-sprockets.js"></script>
    <script type="39c83edf3988fce0c205f478-text/javascript" src="@Url.Content("~/Content/Profile/")js/main.js"></script>
    <script src="@Url.Content("~/Content/Profile/")/ajax.cloudflare.com/cdn-cgi/scripts/a2bd7673/cloudflare-static/rocket-loader.min.js" data-cf-settings="39c83edf3988fce0c205f478-|49" defer=""></script>
    <script src="@Url.Content("~/Content/Medino/assets/")js/vendor/jquery-2.2.4.min.js"></script>
    <script src="@Url.Content("~/Content/Medino/assets/")js/vendor/bootstrap-4.1.3.min.js"></script>
    <script src="@Url.Content("~/Content/Medino/assets/")js/vendor/wow.min.js"></script>
    <script src="@Url.Content("~/Content/Medino/assets/")js/vendor/owl-carousel.min.js"></script>
    <script src="@Url.Content("~/Content/Medino/assets/")js/vendor/jquery.datetimepicker.full.min.js"></script>
    <script src="@Url.Content("~/Content/Medino/assets/")js/vendor/superfish.min.js"></script>
    <script src="@Url.Content("~/Content/Medino/assets/")js/main.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/jquery-3.2.1.min.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/popper.min.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/bootstrap.min.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/jquery.slimscroll.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/select2.min.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/app.js"></script>

</body>

</html>
